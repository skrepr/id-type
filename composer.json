{
    "name": "skrepr/id-type",
    "description": "Symfony bundle for generating and validating ID types",
    "type": "symfony-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "skrepr"
        },
        {
            "name": "Michael Erkens",
            "email": "michael@skrepr.com"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.10",
        "symfony/uid": "^6.2",
        "symfony/framework-bundle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "roave/security-advisories": "dev-latest",
        "skrepr/dto-tester": "^0.2",
        "symfony/maker-bundle": "^1.48",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/var-dumper": "^6.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Skrepr\\IdType\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skrepr\\Tests\\": "tests",
            "Skrepr\\Example\\": "example"
        }
    }
}
